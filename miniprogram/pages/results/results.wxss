.log-list {
  display: flex;
  flex-direction: column;
  padding: 40rpx;
}
.log-item {
  margin: 10rpx;
}
.results {
  margin-left: 0rpx;
  padding-left: 0rpx;
}

.section{
  margin-left: 30rpx;
  font-size: 30rpx;
  padding-bottom: 15rpx;
  color: #2440b3
}

.filter {
  margin-top: 10rpx;
  font-size: 35rpx;
  color: #0d0d0e
}
.Content {
  margin-top: 10rpx;
  margin-bottom: 30rpx;
  font-weight: 200;
  font-size: 30rpx;
  color: #333333;
}

.Title
{
  color: #2440b3
}

.SubDetails 
{
  font-size: 18rpx;
  color: #333333;
}

add-wrap {
  position:fixed;
  top:10%;
  left: 20%;
  width:50%;
}

.add-wrap image {
  width:50px;
  height: 50px;
}

.round-click{
  height: 120rpx;
  width: 120rpx;
  border-radius: 100%;
  position: fixed;
  bottom: 150rpx;
  right: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}
.round-click navigator{
  font-size: 32rpx;
  max-width: 80rpx;
  max-height: 90rpx;
  color: rgb(238, 224, 224);
  text-align: center;
}

.share { 
  height: 140rpx;
  width: 140rpx;
  position: fixed;
  bottom: 170rpx;
  right: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  background: rgba(0, 0, 0, .05);
  border-radius: 50%;
  z-index: 10;
} 
.share image {
  height: 60rpx;
  width: 60rpx;
}
.share text {
  color: #000;  
}
.share button {
  position: absolute;
  height: 100%;
  width: 100%;
  opacity: 0.1;
  z-index: 99;
}

